    "scripts": {
        "dev": "vite",
        "lint": "eslint .",
        "lint:fix": "eslint --fix --cache",
        "build:exp": "tsc -b && vite build",
        "build": "tsc && vite build",
        "preview": "vite preview",
        "analyze": "source-map-explorer 'dist/assets/*.js'",
        "format": "prettier --write --cache src/** --ignore-path .prettierignore",
        "translations-scan": "i18next-scanner --config i18next-scanner.config.engine.cjs",
        "test": "vitest",
        "test:ci": "vitest run",
        "typecheck": "tsc --noEmit",
        "test:coverage": "vitest run --coverage",
        "test:coverage:html": "vitest run --coverage --reporter=html",
        "test:coverage:json": "vitest run --coverage --reporter=json --outputFile=test-results.json",
        "preview:test": "vite preview --outDir html",
        "prebuild": "node -e \"require('fs').rmSync('dist',{recursive:true,force:true})\"",
        "prepare": "husky",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "**/*.{ts,tsx}": [
            "prettier --write",
            "eslint --fix --cache",
            "git add"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
