    "scripts": {
        "start": "set PORT=3006 && react-scripts start",
        "lint": "eslint --fix --cache",
        "lint:fix": "eslint --fix --cache --fix",
        "build": "react-scripts build",
        "analyze": "source-map-explorer 'build/static/js/*.js'",
        "deploy": "aws s3 sync build/ s3://taiger-file-storage-production-website",
        "invalid": "aws cloudfront create-invalidation --distribution-id E31WYFO0IE8GZL --paths \"/*\"",
        "prod": "npm run-script build && npm run-script deploy && npm run-script invalid",
        "format": "prettier --write --cache src/** --ignore-path .prettierignore",
        "prettier": "prettier --write --cache src/** --ignore-path .prettierignore",
        "translations-scan": "i18next-scanner --config i18next-scanner.config.engine.js",
        "test": "react-scripts test",
        "test:ci": "react-scripts test --ci --watchAll=false",
        "eject": "react-scripts eject",
        "prebuild": "rm -rf build",
        "prepare": "husky",
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "**/*.js": [
            "prettier --write",
            "eslint --fix --cache",
            "git add"
        ]
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "devDependencies": {
